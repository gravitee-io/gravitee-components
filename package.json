    "assets/css",
    "assets/icons/gravitee",
    "assets/images",
    "src",
    "wc/gv-*.js"
  ],
  "scripts": {
    "build": "npm run generate:custom-properties && npm run docs && node tasks/check-i18n.js && build-storybook",
    "check:i18n": "node tasks/check-i18n.js",
    "docs": "wca analyze src/** --format markdown --outDir .docs",
    "generate:custom-properties": "node tasks/generate-custom-properties.js",
    "generate:dist": "node tasks/generate-dist.js",
    "generate:icons": "node tasks/generate-icons.js && npm run lint:fix",
    "lint": "eslint src stories tasks",
    "lint:fix": "eslint --fix src stories tasks",
    "start": "npm run docs && start-storybook -p 6006 --ci -s assets/images",
    "start:prod": "npm run build && static -a 0.0.0.0 -p 8080 storybook-static"
  },
